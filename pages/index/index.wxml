<!--index.wxml-->
<view>
  <map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="16" 
            controls="{{controlls}}"
            bindcontroltap="controltap"
            markers="{{markers}}"  
            bindmarkertap="markertap"  
            polyline="{{polyline}}" 
            bindregionchange="regionchange" 
            show-location style="width: 100%; height: 800px;">
  </map>
</view>
